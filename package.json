{
  "name": "angular-globalize-app",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201 --open",
    "build": "ng build angular-globalize --configuration production",
    "test": "ng test angular-globalize",
    "testapp": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --port 4201",
    "build:schemas": "tsc -p ./projects/angular-globalize/tsconfig.schematics.json",
    "copy:schemas": "cpy schematics/**/schema.json ../../dist/angular-globalize/ --cwd projects/angular-globalize --parents",
    "copy:files": "cpy schematics/**/*.template ../../dist/angular-globalize/ --cwd projects/angular-globalize --parents",
    "copy:collection": "cpy schematics/collection.json ../../dist/angular-globalize/ --cwd projects/angular-globalize --parents",
    "copy:readme": "cpy README.md ./dist/angular-globalize",
    "copy:license": "cpy LICENSE ./dist/angular-globalize",
    "copy:all": "yarn copy:schemas && yarn copy:files && yarn copy:collection && yarn copy:readme && yarn copy:license",
    "postbuild": "yarn build:schemas && yarn copy:all",
    "cultures": "cd ./tools && ts-node generate-culture-names.ts",
    "pub": "cd dist/angular-globalize && yarn publish --registry https://npm.pkg.github.com"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/localize": "^13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "cldr-data": "^36.0.0",
    "globalize": "^1.6.0",
    "rxjs": "~7.5.0",
    "tslib": "2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1303.0",
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular-devkit/core": "^13.3.0",
    "@angular-devkit/schematics": "^13.3.0",
    "@angular-eslint/builder": "^13.1.0",
    "@angular-eslint/eslint-plugin": "^13.1.0",
    "@angular-eslint/eslint-plugin-template": "^13.1.0",
    "@angular-eslint/schematics": "^13.1.0",
    "@angular-eslint/template-parser": "^13.1.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "~13.3.0",
    "@types/globalize": "^0.0.35",
    "@types/jasmine": "~3.8.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/karma-jasmine": "^0.0.31",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "bootstrap": "^5.1.1",
    "cpy-cli": "^3.1.0",
    "eslint": "^8.11.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^38.0.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "iana-tz-data": "^2019.1.0",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jsonc-parser": "^3.0.0",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.3.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.6.2"
  },
  "resolutions": {
    "cldr-data/**/minimist": "~0.2.1"
  }
}
