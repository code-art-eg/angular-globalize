{
  "name": "angular-globalize-app",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201 --open",
    "build": "ng build angular-globalize --configuration production",
    "test": "ng test angular-globalize",
    "testapp": "ng test",
    "lint": "ng lint",
    "build:schemas": "tsc -p ./projects/angular-globalize/tsconfig.schematics.json",
    "copy:schemas": "cpy ./schematics/**/schema.json ../../dist/angular-globalize/schematics/ --cwd=./projects/angular-globalize/",
    "copy:files": "cpy ./schematics/**/*.template ../../dist/angular-globalize/schematics/ --cwd=./projects/angular-globalize/",
    "copy:collection": "cpy ./schematics/collection.json ../../../../dist/angular-globalize/ --cwd=./projects/angular-globalize/",
    "copy:readme": "cpy README.md ./dist/angular-globalize/",
    "copy:license": "cpy LICENSE ./dist/angular-globalize/",
    "copy:all": "yarn copy:schemas && yarn copy:files && yarn copy:collection && yarn copy:readme && yarn copy:license",
    "postbuild": "yarn build:schemas && yarn copy:all",
    "cultures": "cd ./tools && ts-node generate-culture-names.ts",
    "pub": "cd dist/angular-globalize && yarn publish --registry https://npm.pkg.github.com"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/localize": "^17.3.12",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "cldr-data": "^36.0.2",
    "globalize": "^1.7.0",
    "rxjs": "~7.8.1",
    "tslib": "~2.6.3",
    "zone.js": "~0.14.8"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1702.3",
    "@angular-devkit/build-angular": "^17.2.3",
    "@angular-devkit/core": "^17.2.3",
    "@angular-devkit/schematics": "^17.2.3",
    "@angular-eslint/builder": "^17.5.2",
    "@angular-eslint/eslint-plugin": "^17.5.2",
    "@angular-eslint/eslint-plugin-template": "^17.5.2",
    "@angular-eslint/schematics": "^17.5.2",
    "@angular-eslint/template-parser": "^17.5.2",
    "@angular/cli": "^17.2.3",
    "@angular/compiler-cli": "^17.3.12",
    "@angular/language-service": "^17.3.12",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.8.0",
    "@popperjs/core": "^2.11.8",
    "@types/globalize": "^1.5.5",
    "@types/jasmine": "~5.1.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/karma-jasmine": "^0.0.31",
    "@types/node": "^20.14.13",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "bootstrap": "^5.3.3",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.9.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "iana-tz-data": "^2019.1.0",
    "jasmine-core": "~5.1.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jsonc-parser": "^3.3.1",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.3.0",
    "postcss": "8.4.31",
    "ts-node": "~10.9.2",
    "typescript": "~5.4.2"
  }
}
