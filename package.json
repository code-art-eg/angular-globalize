{
  "name": "angular-globalize-app",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201 --open",
    "build": "ng build angular-globalize --configuration production",
    "test": "ng test angular-globalize",
    "testapp": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --port 4201",
    "build:schemas": "tsc -p ./projects/angular-globalize/tsconfig.schematics.json",
    "copy:schemas": "cpy schematics/**/schema.json ../../dist/angular-globalize/ --cwd projects/angular-globalize --parents",
    "copy:files": "cpy schematics/**/*.template ../../dist/angular-globalize/ --cwd projects/angular-globalize --parents",
    "copy:collection": "cpy schematics/collection.json ../../dist/angular-globalize/ --cwd projects/angular-globalize --parents",
    "copy:readme": "cpy README.md ./dist/angular-globalize",
    "copy:license": "cpy LICENSE ./dist/angular-globalize",
    "copy:all": "yarn copy:schemas && yarn copy:files && yarn copy:collection && yarn copy:readme && yarn copy:license",
    "postbuild": "yarn build:schemas && yarn copy:all",
    "cultures": "cd ./tools && ts-node generate-culture-names.ts",
    "pub": "cd dist/angular-globalize && yarn publish --registry https://npm.pkg.github.com"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.5",
    "@angular/common": "~12.2.5",
    "@angular/compiler": "~12.2.5",
    "@angular/core": "~12.2.5",
    "@angular/forms": "~12.2.5",
    "@angular/localize": "^12.2.5",
    "@angular/platform-browser": "~12.2.5",
    "@angular/platform-browser-dynamic": "~12.2.5",
    "@angular/router": "~12.2.5",
    "cldr-data": "^36.0.0",
    "globalize": "^1.6.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1202.5",
    "@angular-devkit/build-angular": "~12.2.5",
    "@angular-devkit/core": "^12.2.5",
    "@angular-devkit/schematics": "^12.2.5",
    "@angular-eslint/builder": "^12.4.1",
    "@angular-eslint/eslint-plugin": "^12.4.1",
    "@angular-eslint/eslint-plugin-template": "^12.4.1",
    "@angular-eslint/schematics": "^12.4.1",
    "@angular-eslint/template-parser": "^12.4.1",
    "@angular/cli": "~12.2.5",
    "@angular/compiler-cli": "~12.2.5",
    "@angular/language-service": "~12.2.5",
    "@types/globalize": "^0.0.35",
    "@types/jasmine": "~3.8.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/karma-jasmine": "^0.0.31",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "bootstrap": "^4.6.0",
    "cpy-cli": "^3.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "iana-tz-data": "^2019.1.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^12.2.1",
    "ts-node": "~8.3.0",
    "typescript": "~4.3.5"
  },
  "resolutions": {
    "cldr-data/**/minimist": "~0.2.1"
  }
}
