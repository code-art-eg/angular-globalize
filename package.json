{
  "name": "angular-globalize-app",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201 --open",
    "build": "ng build angular-globalize --configuration production",
    "test": "ng test angular-globalize",
    "testapp": "ng test",
    "lint": "ng lint",
    "build:schemas": "tsc -p ./projects/angular-globalize/tsconfig.schematics.json",
    "copy:schemas": "cpy schematics/**/schema.json ../../dist/angular-globalize/ --cwd projects/angular-globalize --parents",
    "copy:files": "cpy schematics/**/*.template ../../dist/angular-globalize/ --cwd projects/angular-globalize --parents",
    "copy:collection": "cpy schematics/collection.json ../../dist/angular-globalize/ --cwd projects/angular-globalize --parents",
    "copy:readme": "cpy README.md ./dist/angular-globalize",
    "copy:license": "cpy LICENSE ./dist/angular-globalize",
    "copy:all": "yarn copy:schemas && yarn copy:files && yarn copy:collection && yarn copy:readme && yarn copy:license",
    "postbuild": "yarn build:schemas && yarn copy:all",
    "cultures": "cd ./tools && ts-node generate-culture-names.ts",
    "pub": "cd dist/angular-globalize && yarn publish --registry https://npm.pkg.github.com"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.3",
    "@angular/common": "^16.2.3",
    "@angular/compiler": "^16.2.3",
    "@angular/core": "^16.2.3",
    "@angular/forms": "^16.2.3",
    "@angular/localize": "^16.2.3",
    "@angular/platform-browser": "^16.2.3",
    "@angular/platform-browser-dynamic": "^16.2.3",
    "@angular/router": "^16.2.3",
    "cldr-data": "^36.0.0",
    "globalize": "^1.6.0",
    "rxjs": "~7.5.0",
    "tslib": "2.3.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1602.1",
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular-devkit/core": "^16.2.1",
    "@angular-devkit/schematics": "^16.2.1",
    "@angular-eslint/builder": "^16.1.1",
    "@angular-eslint/eslint-plugin": "^16.1.1",
    "@angular-eslint/eslint-plugin-template": "^16.1.1",
    "@angular-eslint/schematics": "^16.1.1",
    "@angular-eslint/template-parser": "^16.1.1",
    "@angular/cli": "^16.2.1",
    "@angular/compiler-cli": "^16.2.3",
    "@angular/language-service": "^16.2.3",
    "@popperjs/core": "^2.11.8",
    "@types/globalize": "^0.0.35",
    "@types/jasmine": "~4.0.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/karma-jasmine": "^0.0.31",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "bootstrap": "^5.3.1",
    "cpy-cli": "^3.1.0",
    "eslint": "^8.39.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^46.5.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "iana-tz-data": "^2019.1.0",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jsonc-parser": "^3.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^16.2.2",
    "postcss": "8.4.31",
    "ts-node": "~8.3.0",
    "typescript": "~4.9.5"
  },
  "resolutions": {
    "cldr-data/**/minimist": "~0.2.1",
    "autoprefixer": "10.4.14"
  }
}
